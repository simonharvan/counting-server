extends layout
block content
    h1 Records
    p This is list of all records
    table
        tr
            th id
            th lat
            th long
            th entrances
            th exits
            th created at
            th vehicle id
        for record in records
            tr
                td= record.id
                td= record.lat
                td= record.lng
                td= record.entr
                td= record.exits
                td= record.createdAt
                td
                    a(href='/vehicle/' + record.VehicleId) #{record.VehicleId}